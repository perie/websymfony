        <script src="{{asset('ressources/public/raphael/raphael-min.js') }}"></script>
        <script src="{{asset('ressources/public/morrisjs/morris.min.js') }}"></script>
        <script src="{{asset('ressources/public/js/morris-data.js') }}"></script>
        <!-- DataTables JavaScript -->
        <script src="{{asset('ressources/public/DataTables/media/js/jquery.dataTables.min.js') }}"></script>
        <script src="{{asset('ressources/public/datatables-plugins/integration/bootstrap/3/dataTables.bootstrap.min.js') }}"></script>
        <!-- Custom Theme JavaScript -->
        <script src="{{asset('ressources/public/js/sb-admin-2.js')}}"></script>
        
        {% block javascripts %}{% endblock %}
