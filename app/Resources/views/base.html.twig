        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link href="{{ asset('css/bootstrap.css')}}" rel="stylesheet">
        <!-- Bootstrap Core CSS -->
        <link href="{{ asset('/ressources/public/bootstrap/dist/css/bootstrap.min.css')}}" rel="stylesheet">
        <!-- Bootstrap Social CSS -->
        <link href="{{ asset('/ressources/public/bootstrap-social/bootstrap-social.css')}}" rel="stylesheet">
