                        <li>|</li>
                        <li><a href="">Utilisateur</a></li>
                        <li>|</li>
                        <li>&nbsp;Nouveau</li>
                    </ul>
                </nav>
            #}
            <div id="page-wrapper">
                
                {% block body %}

                {% endblock %}
            </div>
        </div>
            
        
        <script src="http://code.jquery.com/jquery.js"></script>
        <script src="{{asset('js/bootstrap.js') }}"></script>
        <!-- jQuery -->
        <script src="{{asset('ressources/public/jquery/dist/jquery.min.js') }}"></script>
        <!-- Bootstrap Core JavaScript -->
        <script src="{{asset('ressources/public/bootstrap/dist/js/bootstrap.min.js') }}"></script>
