        <!-- Custom CSS -->
        <link href="{{ asset('/ressources/public/css/sb-admin-2.css')}}" rel="stylesheet">
        <!-- Morris Charts CSS -->
        <link href="{{ asset('/ressources/public/morrisjs/morris.css')}}" rel="stylesheet">
        <!-- Custom Fonts -->
        <link href="{{ asset('/ressources/public/font-awesome/css/font-awesome.min.css')}}" rel="stylesheet" type="text/css">
        <!-- Icon Fonts -->
        <link href="http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css" rel="stylesheet" type="text/css">
    </head>
    
    <body>
